{
  "name": "webpack-react-skeleton",
  "version": "0.3.0",
  "description": "Keathley's skeleton for building apps with react and webpack",
  "main": "bundle.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "test": "mocha --require app/__tests__/test_helper.js --compilers jsx?:babel-core/register 'app/**/*_test.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keathley/webpack-react-skeleton.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "postcss",
    "react",
    "eslint",
    "skeleton"
  ],
  "author": "Chris Keathley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keathley/webpack-react-skeleton/issues"
  },
  "homepage": "https://github.com/keathley/webpack-react-skeleton",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "history": "^1.15.0",
    "immutable": "^3.7.5",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.0",
    "css-modules-require-hook": "^2.1.0",
    "css-mqpacker": "^4.0.0",
    "csswring": "^4.1.1",
    "enzyme": "^1.0.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "mocha": "^2.3.4",
    "postcss": "^5.0.12",
    "postcss-color-function": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "postcss-modules-values": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.0.0",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.9.0"
  }
}
